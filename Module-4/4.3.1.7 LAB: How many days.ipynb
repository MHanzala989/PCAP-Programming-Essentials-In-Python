def days_in_month(year, month):
    if month > 12:
        return None
    month_days = [31,28,31,30,31,30,31,31,30,31,30,31,29]
    if is_leap_year(year) and month == 2:
        days = month_days[-1]
    else:
        days = month_days[month-1]
    return days

